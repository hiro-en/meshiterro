<h2>プロフィール編集</h2>
<%= form_with model: @user do |form| %>
  <label for="input Name">Name</label>
  <%= form.text_field :name, autofocus: true, id:"inputName", placeholder:"名前" %>
  <label for="input Image">ProfileImage</label>
  <%= form.file_field :profile_image, placeholder:"プロフィール画像", accept:"image/*" %>
  <%= form.submit "変更を保存" %>
<% end %>
